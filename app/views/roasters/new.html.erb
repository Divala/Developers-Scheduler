<h1>New roaster</h1>

<% form_for(@roaster) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :developers %><br />
    <%= f.text_field :developers %>
  </p>
  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', roasters_path %>